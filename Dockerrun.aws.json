{
    "AWSEBDockerrunVersion" : 2,
    "containerDefinitions": [
        {
            "name":"client",
            "image":"hahemsi/multi-client",
            "hostname":"client",
            "essential": false
        },
        {
            "name":"server",
            "image":"hahemsi/multi-server",
            "hostname":"api",
            "essential": false
        },
        {
            "name":"worker",
            "image":"hahemsi/multi-worker",
            "hostname":"worker",
            "essential": false
        },
        {
            "name":"nginx",
            "image":"hahemsi/multi-nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": [
                "client",
                "server"
            ]
        }
    ]

}